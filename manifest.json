{
  "name": "Blockr",
  "version": "1.0",
  "description": "Be a producer, not a consumer.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    },
  "permissions": ["webRequest", "webRequestBlocking", "*://*/*"],
  "background": {
    "scripts": ["jquery.js", "blockr.js"]
  },
  "manifest_version": 2
}
